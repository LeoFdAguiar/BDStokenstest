        }
      },
      "attention": {
        "accentcolorprimary": {
          "light": {
            "value": "{Global.Attention.accentColorPrimary}",
            "comment": "Primary accent color used to emphasize interactive or highlight elements such as links, selection states, or focus indicators. Adapts in Darkmode to a fully white tone."
          }
        },
        "accentcolorprimaryconstant": {
          "light": {
            "value": "{Global.Attention.accentColorPrimary}",
            "comment": "Used for accent highlights that must remain visually consistent across themes (e.g., brand identifiers, logos, or fixed emphasis areas). Color value does not change between Light and Dark mode."
          }
        },
        "attentioncolorsuccessprimary": {
          "light": {
            "value": "{BILD.green.BILDGreen070}",
            "comment": "Primary success color used to indicate positive states, confirmations, or completed actions (e.g., success banners, icons, or badges). Remains the same across modes to ensure recognizability and consistency."
          }
        },
        "attentioncolorsuccesssecondary": {
          "light": {
            "value": "{BILD.green.BILDGreen040}",
            "comment": "Secondary success tone used for secondary states. Maintains the same color in both Light and Dark mode for clarity."
          }
        },
        "attentioncolorextralow": {
          "light": {
            "value": "{BILD.neutral.BILD100}",
            "comment": "Very low-contrast background tone used for information or system-neutral surfaces, often for tooltips or quiet information highlights. Dynamic — light neutral in Light mode and dark neutral in Dark mode."
          }
        },
        "attentioncolorlow": {
          "light": {
            "value": "{BILD.yellow.BILDYellow}",
            "comment": "Low-level warning or attention tone, typically used for informational or cautionary messages. Same yellow tone across Light and Dark to maintain recognition."
          }
        },
        "attentioncolormedium": {
          "light": {
            "value": "{BILD.orange.BILDOrange}",
            "comment": "Medium-level attention tone representing stronger caution or intermediate alert states. Commonly applied in warning messages. Constant — identical in both modes."
          }
        },
        "attentioncolorhigh": {
          "light": {
            "value": "{BILD.red.BILDRed}",
            "comment": "High-level attention color used for errors, destructive actions, and critical alerts (e.g., delete actions, error states). Remains red in both Light and Dark mode for immediate recognition."
          }
        }
      },
      "border": {
        "bordercolorlowcontrast": {
          "light": {
            "value": "{BILD.gray.BILD093}",
            "comment": "Used for subtle dividers and low-emphasis borders in neutral areas (e.g., card outlines, input containers).\nDynamic — light gray in Light mode, dark gray in Dark mode."
          }
        },
        "bordercolorlowcontrastconstant": {
          "light": {
            "value": "{BILD.gray.BILD093}",
